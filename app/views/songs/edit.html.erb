<%= form_for(@song) do |f| %>
<%= f.text_field :title %><br><br>
<%= f.label "Released?" %>
<%= f.label "Yes" %>
<%= f.radio_button :released, true  %>

<%= f.label "No" %>
<%= f.radio_button :released, false %><br><br>

<%= f.number_field :release_year %><br><br>

<%= f.text_field :artist_name %><br><br>

<%= f.text_field :genre %><br><br>

<%= f.submit %>

<% end %>
